{
  "tasks": {
    "dev": "deno run --watch --allow-env --allow-sys --allow-read --allow-net src/main.ts",
    "start": "deno run --allow-env --allow-sys --allow-read --allow-net src/main.ts"
  },
  "imports": {
    "libs": "./src/libs/index.ts",
    "utils": "./src/utils/index.ts",
    "material": "./src/material/index.ts",
    "generators": "./src/generators/index.ts",
    "compression": "https://esm.sh/compression@1.7.4",
    "cors": "https://esm.sh/cors@2.8.5",
    "chroma-js": "https://esm.sh/chroma-js@2.4.2",
    "express": "https://esm.sh/express@4.18.2",
    "express-serve-static-core": "https://esm.sh/express-serve-static-core@0.1.1",
    "@material/material-color-utilities": "https://esm.sh/@material/material-color-utilities@0.2.7"
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "indentWidth": 2,
    "semiColons": false,
    "singleQuote": true,
    "proseWrap": "preserve",
    "include": [
      "src/"
    ],
    "exclude": [
      "src/testdata/",
      "data/fixtures/**/*.ts"
    ]
  }
}
